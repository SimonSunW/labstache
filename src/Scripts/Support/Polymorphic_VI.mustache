<!DOCTYPE HTML PUBLIC "-//w3c//dtd html 4.01 transitional//en">
<html>
<head>
<title>{{name}}</title>
<meta name="author" content="Field R&D Services, LLC">
<meta name="generator" content="G Development Environment">
<meta http-equiv="Content-Type" content="text/html; charset=Windows-1252">
<meta http-equiv="Content-Style-Type" content="text/css">
<link rel="stylesheet" type="text/css" href="/styles/minimal.css">
<script src="/scripts/jquery-1.7.2.min.js" type="text/javascript"></script>
<script src="/scripts/cssframes.js" type="text/javascript"></script>
<link media="print" rel="stylesheet" type="text/css" href="/styles/print.css">
</head>
<body bgcolor="#ffffff">

<div id="styleDivTitle">
<h1>{{name}}</h1>
<p class="Body"><strong>Owning Palette:</strong> <a href="{{owning-palette-url}}">{{owning-palette-name}}</a></p>
<p class="Body"><strong>Requires:</strong> {{requirement}}</p>
<p class="Body">{{description}}</p>
<p class="Anchor"><img src="{{description-image-src}}" alt="{{name}}"></p>
<table class="Borderless">
<tr>
<td>
<select name="site" size="1" onchange="polyVISelectHandler(this)">
<option value="">Select an instance</option>
{{#children}}
<option value="{{selection-value}}">{{selection-label}}</option>
{{/children}}
</select>
</td>
</tr>
</table>
</div>

<div id="styleDivScrolling">
{{#children}}
<h2><a name="{{selection-value}}">{{selection-label}}</a></h2>
<p class="Anchor"><img src="{{selection-image-src}}" alt="{{selection-label}}"></p>
<table class="Borderless">
{{#controls-and-indicators}}
<tr>
<td class="Icon"><img src="{{control-or-indicator-image-src}}" alt="{{control-or-indicator-image-alt}}"></td>
<td>{{{control-or-indicator-description}}}</td>
</tr>
{{/controls-and-indicators}}
</table>
{{/children}}
</div>

</body>
</html>
